{
  "manifest_version": 2,

  "name": "nabi",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "Nabi_Logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["test.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
	"tabs",
	"windows",
    "https://ajax.googleapis.com/",
	"contentSettings"
  ]
}
